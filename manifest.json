
{
  "name": "One simple Git extension",
  "description": "git quick review & merge pull request",
  "version": "2",
  "permissions": [
    "activeTab"
  ],
  // "background": {
  //   "scripts": ["click.js"]
  // },
  "content_scripts":[
    {
      "matches": ["http://github.com/*ExtensionTest/pull*"],
      "js": ["click.js"]
    }
  ],
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
